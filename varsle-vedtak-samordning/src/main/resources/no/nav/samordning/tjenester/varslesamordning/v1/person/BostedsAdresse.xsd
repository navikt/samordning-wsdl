<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:bons0="http://nav.no/samordning/tjenester/varslesamordning/v1/person"
            targetNamespace="http://nav.no/samordning/tjenester/varslesamordning/v1/person">
    <xsd:include schemaLocation="MatrikkelAdresse.xsd"/>
    <xsd:include schemaLocation="OffentligAdresse.xsd"/>
    <xsd:complexType name="BostedsAdresse">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="boadresse1" type="xsd:string"/>
            <xsd:element minOccurs="0" name="boadresse2" type="xsd:string"/>
            <xsd:element minOccurs="0" name="bolignr" type="xsd:string"/>
            <xsd:element minOccurs="0" name="postnr" type="xsd:string"/>
            <xsd:element minOccurs="0" name="poststed" type="xsd:string"/>
            <xsd:element minOccurs="0" name="kommunenr" type="xsd:string"/>
            <xsd:element minOccurs="0" name="navEnhet" type="xsd:string"/>
            <xsd:element minOccurs="0" name="fom" type="xsd:date"/>
            <xsd:element minOccurs="0" name="offentligAdresse" type="bons0:OffentligAdresse"/>
            <xsd:element minOccurs="0" name="matrikkelAdresse" type="bons0:MatrikkelAdresse"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
